{
  "rewrites": [
    {
      "source": "/_mintlify/:path*",
      "destination": "https://vssh.mintlify.app/_mintlify/:path*"
    },
    {
      "source": "/api/request",
      "destination": "https://vssh.mintlify.app/api/request"
    },
    {
      "source": "/docs",
      "destination": "https://vssh.mintlify.app"
    },
    {
      "source": "/docs/llms.txt",
      "destination": "https://vssh.mintlify.app/llms.txt"
    },
    {
      "source": "/docs/llms-full.txt",
      "destination": "https://vssh.mintlify.app/llms-full.txt"
    },
    {
      "source": "/docs/sitemap.xml",
      "destination": "https://vssh.mintlify.app/sitemap.xml"
    },
    {
      "source": "/docs/robots.txt",
      "destination": "https://vssh.mintlify.app/robots.txt"
    },
    {
      "source": "/docs/mcp",
      "destination": "https://vssh.mintlify.app/mcp"
    },
    {
      "source": "/docs/:path*",
      "destination": "https://vssh.mintlify.app/:path*"
    },
    {
      "source": "/api-reference/:path*",
      "destination": "https://vssh.mintlify.app/api-reference/:path*"
    },
    {
      "source": "/guides/:path*",
      "destination": "https://vssh.mintlify.app/guides/:path*"
    },
    {
      "source": "/authentication",
      "destination": "https://vssh.mintlify.app/authentication"
    },
    {
      "source": "/mintlify-assets/:path*",
      "destination": "https://vssh.mintlify.app/mintlify-assets/:path*"
    }
  ]
}
